{
  "name": "giffy.club",
  "version": "1.0.0",
  "description": "Public website for giffy.club",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf .bin",
    "client": "mkdir -p .bin/public/{css,js} && taunus -o && browserify client/js/main.js -do .bin/public/js/all.js && stylus client/css/all.styl -o .bin/public/css --include-css && cp -r node_modules/font-awesome-stylus/fonts .bin/public",
    "start": "npm run clean && npm run views && npm run client && node app.js",
    "views": "hoganator --outputdir .bin views/*.html views/**/*.html"
  },
  "author": "Nicolas Bevacqua <nicolasbevacqua@gmail.com>",
  "license": "MIT",
  "engines": {
    "iojs": "2.2.1"
  },
  "dependencies": {
    "body-parser": "^1.13.1",
    "browserify": "^10.2.4",
    "debug": "2.2.0",
    "dominus": "^5.0.0",
    "express": "^4.13.0",
    "font-awesome-stylus": "4.3.0",
    "gradual": "1.0.2",
    "hogan.js": "^3.0.2",
    "hoganator": "^1.0.0",
    "lodash": "3.10.0",
    "nib": "1.1.0",
    "normalize.css": "3.0.3",
    "random-word": "1.0.2",
    "request": "2.58.0",
    "serve-static": "^1.10.0",
    "skyrocket": "1.0.0",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "stylus": "0.51.1",
    "taunus": "^6.1.6",
    "taunus-express": "^3.0.0",
    "titlecase": "1.0.2"
  }
}
