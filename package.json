{
  "name": "gifcito",
  "version": "1.0.0",
  "description": "Gifcito.com",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf .bin",
    "client": "mkdir -p .bin/public/js && taunus -o && browserify client/js/main.js -do .bin/public/js/all.js",
    "start": "npm run clean && npm run views && npm run client && node app.js",
    "views": "hoganator --outputdir .bin views/*.html views/**/*.html"
  },
  "author": "Nicolas Bevacqua <nicolasbevacqua@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.1",
    "browserify": "^10.2.4",
    "debug": "2.2.0",
    "dominus": "^5.0.0",
    "express": "^4.13.0",
    "gradual": "1.0.2",
    "hogan.js": "^3.0.2",
    "hoganator": "^1.0.0",
    "lodash": "3.10.0",
    "random-word": "1.0.2",
    "request": "2.58.0",
    "serve-static": "^1.10.0",
    "skyrocket": "1.0.0",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "taunus": "^6.1.6",
    "taunus-express": "^3.0.0",
    "titlecase": "1.0.2"
  }
}
